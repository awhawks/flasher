<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="demo" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="rls_win32">
				<Option platforms="Windows;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option type="1" />
				<Option compiler="msvc8" />
				<Compiler>
					<Add option="/W3" />
					<Add option="/DWIN32" />
					<Add directory="../libmmc" />
				</Compiler>
				<Linker>
					<Add library="libmmc" />
					<Add directory="../libmmc" />
				</Linker>
			</Target>
			<Target title="rls_linux">
				<Option platforms="Unix;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-DLINUX" />
					<Add directory="../libmmc" />
				</Compiler>
				<Linker>
					<Add option="-lmmc" />
					<Add library="libmmc" />
					<Add directory="../libmmc" />
				</Linker>
			</Target>
			<Target title="rls_macosx">
				<Option platforms="Mac;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-x c -arch i386 -isysroot /Developer/SDKs/MacOSX10.4u.sdk -mmacosx-version-min=10.4" />
					<Add directory="../libmmc" />
				</Compiler>
				<Linker>
					<Add option="-framework IOKit -framework CoreServices" />
					<Add library="libmmc" />
					<Add directory="../libmmc" />
				</Linker>
			</Target>
			<Target title="dbg_win32">
				<Option platforms="Windows;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="msvc8" />
				<Compiler>
					<Add option="/W3" />
					<Add option="/Zi /D_DEBUG" />
					<Add option="/DWIN32" />
				</Compiler>
				<Linker>
					<Add option="/DEBUG" />
					<Add library="libmmc.lib" />
				</Linker>
			</Target>
			<Target title="dbg_linux">
				<Option platforms="Unix;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Linker>
					<Add option="-lmmc" />
				</Linker>
			</Target>
			<Target title="dbg_macosx">
				<Option platforms="Mac;" />
				<Option output="demo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-x c -arch i386 -isysroot /Developer/SDKs/MacOSX10.4u.sdk -mmacosx-version-min=10.4" />
					<Add option="-DMACOSX" />
					<Add directory="../libmmc" />
				</Compiler>
				<Linker>
					<Add option="-framework IOKit -framework CoreServices" />
					<Add library="libmmc" />
					<Add directory="../libmmc" />
				</Linker>
			</Target>
		</Build>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
